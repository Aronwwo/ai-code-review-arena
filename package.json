{
  "name": "ai-code-review-arena",
  "version": "1.0.0",
  "description": "Multi-agent AI code review with debate capabilities",
  "scripts": {
    "setup": "node scripts/setup.js",
    "dev": "node scripts/dev.js",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && uvicorn app.main:app --reload --host 0.0.0.0 --port 8000",
    "test": "cd backend && python3 -m pytest tests/ -v",
    "test:e2e": "playwright test",
    "test:frontend": "cd frontend && npm test",
    "build": "cd frontend && npm run build",
    "lint": "cd frontend && npm run lint",
    "seed": "cd backend && venv\\Scripts\\python scripts/seed_admin.py"
  },
  "keywords": ["ai", "code-review", "llm", "ollama", "fastapi", "react"],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.2"
  }
}
